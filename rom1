function [13:0] rom1;//í‹€
	input [3:0] addr_in;
	
	begin 
		case (addr_in)
			0: rom1 = 14'b11111111111110;
			1: rom1 = 14'b10001000100010;
			2: rom1 = 14'b10001000100010;
			3: rom1 = 14'b11111111111110;
			4: rom1 = 14'b10001000100010;
			5: rom1 = 14'b10001000100010;
			6: rom1 = 14'b11111111111110;
			7: rom1 = 14'b10001000100010;
			8: rom1 = 14'b10001000100010;
			9: rom1 = 14'b11111111111110;
		default: rom1 = 14'b00000000000000;
		endcase
	end
endfunction
